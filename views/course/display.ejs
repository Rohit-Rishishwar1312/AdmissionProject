<%- include('../header.ejs') %>

<div class="container mt-5">
    <table class="table table-bordered table-striped tabl">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Action</th>
        </tr>
        <% d.forEach((item)=>{ %>
            <tr>
                <td><%= item._id %></td>
                <td><%= item.name %></td>
                <td><%= item.email %></td>
                <td><%= item.phone %></td>
                <td>
                    <a href="/view/<%= item._id %>" class="btn btn-info">view</a>
                    <a href="/edit/<%= item._id %>" class="btn btn-success">edit</a>
                    <a href="" class="btn btn-danger">delete</a>
                </td>
            </tr>

        <% }) %>
        
    </table>
</div>

<%- include('../footer.ejs') %>
