<%- include('../header.ejs') %>

<div class="container mt-5">
    <table class="table table-bordered table-responsive table-striped tabl">
        
            <th colspan="6" class="text-center text-black">Courses you have enrolled</th>
        <tr>
            <th>S.No</th>
            <th>Name</th>
            <th>Email</th>
            <th>Course</th>
            <th>Status</th>
            <th>Comment</th>
            <th>Action</th>
        </tr>
        <% number=1 %>
        <% d.forEach((item)=>{ %>
            <tr>
                <td><%= number++ %></td>
                <td><%= item.name %></td>
                <td><%= item.email %></td>
                <td><%= item.course %></td>
                <td><%= item.status %></td>
                <td><%= item.comment %></td>
                <td>
                    <a href="/view/<%= item._id %>" class="btn btn-info">view</a>
                    <a href="/edit/<%= item._id %>" class="btn btn-success">edit</a>
                    <!-- <a href="/delete/<%= item._id %>" class="btn btn-danger">delete</a> -->
                </td>
            </tr>

        <% }) %>
        
    </table>
</div>

<%- include('../footer.ejs') %>
